{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./"
  },
  "functions": {
    "vercel_app.py": {
      "includeFiles": "templates/**",
      "maxDuration": 30,
      "memory": 512
    }
  },
  "buildCommand": "echo 'Using vercel_app.py as main entry point'"
}
