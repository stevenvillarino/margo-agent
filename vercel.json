{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./"
  },
  "functions": {
    "vercel_app.py": {
      "includeFiles": "templates/**",
      "maxDuration": 30,
      "memory": 512
    }
  },
  "framework": null,
  "installCommand": "pip install -r requirements.txt"
}
